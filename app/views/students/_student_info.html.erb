<table class="class_form">
      <tr>
        <th>Last Name</th>
        <th>First Name</th>
        <th>Stony Brook Id</th>
        <th>Host Institution</th>
      </tr>

    <% @students.each do |student| %>
      <tr>
        <td><%=h student.last_name %></td>
        <td><%=h link_to student.first_name, :action => "class_information", :student_id => student.id %></td>
        <td><%=h student.stony_brook_id %></td>
        <td><%=h student.host_institution %></td>
      </tr>
    <% end %>
</table>
<%= will_paginate @students %>

<br />