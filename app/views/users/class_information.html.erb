<h1>Listing class Informations</h1>
 
<table class="class_form">
  <tr>
    <th>Course Title</th>
    <th>Class Number</th>
    <th>Section Number</th>
    <th>Credits</th>
    <th>Day</th>
    <th>Start Time</th>
    <th>Stop Time</th>
  </tr>

<% @class_information.each do |info| %>
  <tr>
    <td><%=h info.title %></td>
    <td><%=h info.class_number %></td>
    <td><%=h info.section_number %></td>
    <td><%=h info.credits %></td>
    <td><%=h info.day %></td>
    <td><%=h (info.start_time).strftime("%I:%M %p") %></td>
    <td><%=h (info.stop_time).strftime("%I:%M %p") %></td>    
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Back', :controller => 'users' %>